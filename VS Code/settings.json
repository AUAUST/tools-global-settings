{
  "[blade]": {
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[denizenscript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[liquid]": {
    "editor.formatOnSave": false
  },
  "[mdx]": {
    "editor.defaultFormatter": "unifiedjs.vscode-mdx"
  },
  "[nginx]": {
    "editor.defaultFormatter": "ahmadalli.vscode-nginx-conf"
  },
  "[php]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    },
    "editor.defaultFormatter": "DEVSENSE.phptools-vscode",
    "editor.formatOnSave": true,
    "editor.indentSize": 4
  },
  "[plaintext]": {
    "editor.wordWrap": "on"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "colorful-comments.tags": [
    {
      "backgroundColor": "transparent",
      "color": "#bc4b4b",
      "strikethrough": false,
      "tag": "!"
    },
    {
      "backgroundColor": "transparent",
      "color": "#696cfc",
      "strikethrough": false,
      "tag": "?"
    },
    {
      "backgroundColor": "#170049",
      "color": "#efeff5",
      "strikethrough": false,
      "tag": "AUAUST"
    },
    {
      "backgroundColor": "#170049",
      "color": "#efeff5",
      "strikethrough": false,
      "tag": "* useGlobal"
    },
    {
      "backgroundColor": "#170049",
      "color": "#efeff5",
      "strikethrough": false,
      "tag": "* useLocal"
    },
    {
      "backgroundColor": "#f79f57",
      "color": "#000000",
      "strikethrough": false,
      "tag": "todo"
    }
  ],
  "denizenscript.debug.highlighting": true,
  "denizenscript.theme_colors.bad_space": "#FFFFFF|background=#bc4b4b",
  "denizenscript.theme_colors.colons": "#00bacc|style=bold",
  "denizenscript.theme_colors.command": "#ff8bf3|style=regular",
  "denizenscript.theme_colors.comment_code": "#7fff7f",
  "denizenscript.theme_colors.comment_header": "#f47d78",
  "denizenscript.theme_colors.comment_normal": "#818194",
  "denizenscript.theme_colors.comment_todo": "#fee75c|style=bold",
  "denizenscript.theme_colors.def_name": "#fee75c",
  "denizenscript.theme_colors.key": "#f79f57",
  "denizenscript.theme_colors.key_inline": "#f79f57|style=regular",
  "denizenscript.theme_colors.quote_double": "#8aecf3",
  "denizenscript.theme_colors.quote_single": "#8aecf3",
  "denizenscript.theme_colors.space": "#61616f",
  "denizenscript.theme_colors.tag": "#a1a1ac",
  "denizenscript.theme_colors.tag_dot": "#d951ff",
  "denizenscript.theme_colors.tag_param": "#8aecf3",
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "\"Suisse Int'l Mono\", Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 15.75,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.inlayHints.fontSize": -1,
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.lineHeight": 22,
  "editor.minimap.enabled": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "class": "#7fff7f",
      "comment": "#818192",
      "function": "#ff8bf3",
      "method": "#ff8bf3",
      "namespace": "#00bacc",
      "parameter": "#fee75c",
      "property": "#8aecf3",
      "property.declaration": "#8aecf3",
      "variable": "#fee75c",
      "variable.defaultLibrary": "#8aecf3",
      "variable.readonly": "#f79f57"
    }
  },
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "text",
          "string",
          "string.quoted",
          "string.regexp.js",
          "meta.group.regexp",
          "keyword.other.unit",
          "punctuation.separator.key-value",
          "entity.other.attribute-name.id.css",
          "entity.other.attribute-name.class.css",
          "constant.other.color.rgb-value.hex.css",
          "constant.other.character-class.set.regexp",
          "punctuation.accessor.ts",
          "keyword.operator.class",
          "punctuation.separator.inheritance",
          "punctuation.separator.delimiter"
        ],
        "settings": {
          "foreground": "#efeff5"
        }
      },
      {
        "scope": [
          "markup.bold",
          "entity.other.attribute-name",
          "punctuation.definition.group.regexp",
          "markup.italic",
          "punctuation.definition.tag",
          "markup.placeholder.ponctuation.open_argument",
          "constant.character.format.placeholder.other.python",
          "punctuation.definition.list.begin.markdown"
        ],
        "settings": {
          "foreground": "#a1a1ac"
        }
      },
      {
        "scope": ["comment", "markup.ignored"],
        "settings": {
          "foreground": "#818192"
        }
      },
      {
        "scope": [
          "markup.placeholder",
          "markup.bold.symbol",
          "markup.italic.symbol",
          "markup.heading.symbol",
          "markup.inline.key.symbol",
          "markup.multi_line.key.symbol",
          "punctuation.definition",
          "punctuation.attribute-shorthand.event.html.vue",
          "punctuation.attribute-shorthand.bind.html.vue",
          "punctuation.definition.heading.markdown",
          "markup.heading.setext"
        ],
        "settings": {
          "foreground": "#61616f"
        }
      },
      {
        "scope": [
          "keyword.control",
          "entity.name.tag.control.liquid",
          "entity.name.tag.conditional.liquid",
          "entity.name.tag.iteration.liquid",
          "entity.name.tag.variable.liquid",
          "entity.name.tag.reference.scss",
          "keyword.control.anchor.regexp",
          "markup.inline.key.name",
          "markup.multi_line.key.name",
          "keyword.other",
          "keyword.operator.logical.python"
        ],
        "settings": {
          "foreground": "#d951ff"
        }
      },
      {
        "scope": [
          "entity.name.function",
          "support.function",
          "constant.other.character-class.regexp",
          "keyword.operator.combinator.css",
          "markup.placeholder.function"
        ],
        "settings": {
          "foreground": "#ff8bf3"
        }
      },
      {
        "scope": ["markup.placeholder.syntax_error"],
        "settings": {
          "foreground": "#bc4b4b"
        }
      },
      {
        "scope": [
          "entity.name.tag.wildcard.css",
          "keyword.operator.quantifier.regexp"
        ],
        "settings": {
          "foreground": "#f47d78"
        }
      },
      {
        "scope": [
          "entity.name.tag",
          "variable.other.constant",
          "support.type.property-name"
        ],
        "settings": {
          "foreground": "#f79f57"
        }
      },
      {
        "scope": [
          "variable.other",
          "variable.other.regexp",
          "markup.placeholder.custom",
          "punctuation.output.liquid",
          "support.constant.property-value",
          "punctuation.definition.variable.php",
          "entity.name.type.namespace",
          "meta.property-name.scss"
        ],
        "settings": {
          "foreground": "#fee75c"
        }
      },
      {
        "scope": [
          "support.class",
          "constant.numeric",
          "entity.name.tag.custom.css",
          "entity.name.tag.custom.scss",
          "markup.placeholder.argument",
          "meta.tag.custom.start.html",
          "entity.name.type.class",
          "entity.other.inherited-class",
          "entity.other.alias"
        ],
        "settings": {
          "foreground": "#7fff7f"
        }
      },
      {
        "scope": [
          "variable.language",
          "constant.language",
          "support.constant.media.css",
          "keyword.operator.or.regexp",
          "punctuation.definition.string",
          "punctuation.definition.entity.css",
          "punctuation.definition.constant.css",
          "entity.other.attribute-name.pseudo-element.css",
          "markup.multi_line.limit",
          "keyword.other.type"
        ],
        "settings": {
          "foreground": "#00bacc"
        }
      },
      {
        "scope": [
          "constant.character.escape",
          "markup.placeholder.default",
          "support.other.namespace",
          "keyword.operator.type"
        ],
        "settings": {
          "foreground": "#8aecf3"
        }
      },
      {
        "scope": [
          "support.type.property-name.media.css",
          "entity.name.tag.css"
        ],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": [
          "storage.type",
          "punctuation.section.embedded.end.php",
          "keyword.other.important.css",
          "punctuation.section.embedded.begin.php",
          "punctuation.definition.character-class.regexp",
          "keyword.operator.expression.typeof",
          "markup.placeholder.ponctuation.chaining",
          "storage.modifier.async",
          "keyword.operator.expression",
          "storage.modifier",
          "keyword.other.class"
        ],
        "settings": {
          "foreground": "#696cfc"
        }
      },
      {
        "scope": [
          "markup.heading",
          "markup.placeholder",
          "markup.bold.symbol",
          "markup.italic.symbol"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": ["markup.italic"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["markup.bold"],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["markup.heading.text.1", "heading.1.markdown"],
        "settings": {
          "foreground": "#F47D78"
        }
      },
      {
        "scope": ["markup.heading.text.2", "heading.2.markdown"],
        "settings": {
          "foreground": "#F69272"
        }
      },
      {
        "scope": ["markup.heading.text.3", "heading.3.markdown"],
        "settings": {
          "foreground": "#F8A76D"
        }
      },
      {
        "scope": ["markup.heading.text.4", "heading.4.markdown"],
        "settings": {
          "foreground": "#FABD67"
        }
      },
      {
        "scope": ["markup.heading.text.5", "heading.5.markdown"],
        "settings": {
          "foreground": "#FCD262"
        }
      },
      {
        "scope": ["markup.heading.text.6", "heading.6.markdown"],
        "settings": {
          "foreground": "#FEE75C"
        }
      }
    ]
  },
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": true,
  "explorer.decorations.colors": true,
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.sortOrder": "fullPath",
  "explorer.sortOrder": "type",
  "files.associations": {
    "*.config": "json",
    "*.dsc": "denizenscript",
    "*.env": "dotenv",
    "*.idjs": "javascript",
    "*.ttx": "xml"
  },
  "files.enableTrash": true,
  "files.encoding": "utf8",
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/dist/**": true,
    "**/node_modules/*/**": true,
    "**/vendor/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.decorations.enabled": true,
  "git.enabled": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": true,
    "scminput": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "intelephense.environment.shortOpenTag": false,
  "javascript.format.semicolons": "insert",
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.format.keepLines": true,
  "Laravel.inertia.diagnostics": false,
  "npm.packageManager": "yarn",
  "php.format.codeStyle": "Laravel",
  "php.problems.exclude": {
    "/": [7103, "PHP7103", "7103"]
  },
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.associations.files": [
    {
      "extensions": ["dsc"],
      "format": "svg",
      "icon": "denizenscript"
    }
  ],
  "vsicons.presets.hideFolders": true,
  "workbench.colorCustomizations": {
    "button.background": "#fee75c",
    "button.foreground": "#000000",
    "button.hoverBackground": "#7fff7f",
    "checkbox.border": "#46464f",
    "disabledForeground": "#efeff5aa",
    "dropdown.background": "#46464f",
    "dropdown.border": "#46464f",
    "dropdown.foreground": "#efeff5",
    "dropdown.listBackground": "#46464f",
    "editor.background": "#171719",
    "editor.findMatchBackground": "#7fff7f84",
    "editor.findMatchHighlightBackground": "#7fff7f38",
    "editor.foreground": "#efeff5",
    "editor.hoverHighlightBackground": "#efeff53e",
    "editor.lineHighlightBackground": "#27272a",
    "editor.selectionBackground": "#8aecf3ae",
    "editor.selectionHighlightBackground": "#8aecf338",
    "editorBracketHighlight.foreground1": "#c7b6ac",
    "editorBracketHighlight.foreground2": "#ece6e2",
    "editorBracketHighlight.foreground3": "#947a6a",
    "editorBracketHighlight.unexpectedBracket.foreground": "#f47d78",
    "editorBracketMatch.border": "#f47d78",
    "editorError.background": "#bc4b4b61",
    "editorError.foreground": "#bc4b4b",
    "editorGroupHeader.border": "#61616f",
    "editorGroupHeader.tabsBackground": "#36363b",
    "editorGroupHeader.tabsBorder": "#61616f",
    "editorGutter.addedBackground": "#7fff7f61",
