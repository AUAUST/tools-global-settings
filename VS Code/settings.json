    "editorGutter.modifiedBackground": "#f79f5761",
    "editorInlayHint.background": "#171719",
    "editorInlayHint.foreground": "#61616f",
    "editorLineNumber.activeForeground": "#88889e",
    "editorLineNumber.foreground": "#52525a",
    "editorLink.activeForeground": "#fee75c",
    "editorOverviewRuler.background": "#171719",
